
#include "rcCar.h"
#include "main.h"





void rcCar()
{
	for( uint8_t i =0; i<4 ; i++)
	{
		if (rxdata2 == a)
		{
			TIM3->CCR1 = 125;
		}

		else if(rxdata2 == d)
		{
			TIM3->CCR2 = 125;
		}
		else if(rxdata2 == w)
		{
			TIM3->CCR3 = 125;
		}
		else if(rxdata == s )
		{
			TIM3->CCR4 = 125;
		}
		else
		{
			TIM3->CCR1 = 0;
			TIM3->CCR2 = 0;
			TIM3->CCR3 = 0;
			TIM3->CCR4 = 0;
		}


	}


}
