
#include <stdint.h>

#define RCC_CR_HSEON			(1<<16) //  오실레이터 HSE ON
#define RCC_CR_HSERDY			(1<<17) //	HSE Ready 플래그
#define RCC_CR_PLLON			(1<<24) //	PLL ON
#define RCC_CR_PLLRDY			(1<<25) //	PLL Flag

#define RCC_PLLCFGR_PLLM_Pos	0   	//PLLM position
#define RCC_PLLCFGR_PLLN_Pos	6   	//PLLN position
#define RCC_PLLCFGR_PLLP_Pos	16  		 //PLLP position

int main(void)
{
	while(1)
	{

	}
}
